extends layout

block content
  .container
    header
      h1 
        a(href=feed.feed_url) #{feed.title} 
          span.mega-octicon.octicon-rss 
    .content
      ul.itemlist.list-group
        each item in feed.items
          li.list-group-item #{item.title} 
            button.btn.btn-default.octicon.octicon-playback-play.pull-right.item-play-button(data-url=item.enclosure.url)
    include player
